    "\n",
    "from itertools import combinations\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def find_matches(smarts, mol):\n",
    "  \"\"\"\n",
    "  mol: rdkit molecule\n",
    "  substructs: numpy array of 424 functional group SMARTS patterns`\n",
    "  \"\"\"\n",
    "  matches = []\n",
    "  for idx in range(len(smarts)):\n",
    "    query = smarts[idx]\n",
    "    if isinstance(query[2], list):\n",
    "      for q in query[2]:\n",
    "        item = mol.GetSubstructMatches(Chem.MolFromSmarts(q))\n",
    "        for i in item:\n",
    "          matches.append((i, query[1]))\n",
    "    else:\n",
    "        item = mol.GetSubstructMatches(Chem.MolFromSmarts(query[2]))\n",
    "        if item:\n",
    "          for i in item:\n",
    "            matches.append((i, query[1]))\n",
    "\n",
    "  return matches\n",
    "\n",
    "def find_fragmentation(mol, substructs, smart_cands):\n",
    "  \"\"\"\n",
    "  mol: Rdkit molecule\n",
    "  substructs: numpy array of 424 functional group SMARTS patterns\n",
    "  smart_cands: fg index slice corresponding to the functional groups present in mol\n",
    "  \n",
    "  \"\"\"\n",
    "  heap = []\n",
    "  for i in smart_cands:\n",
    "    heapq.heappush(heap,  (util.group_weights[i], i + 1, smarts[i][1]))\n",
    "\n",
    "  smarts_sorted = np.array(sorted(heap, reverse=True), dtype=\"object\")\n",
    "  results = []\n",
    "  num_atoms_in_mol = mol.GetNumAtoms()\n",
    "  smart_cands = smart_cands.astype(int)\n",
    "  matches = find_matches(smarts_sorted, mol)\n",
    "\n",
    "  for i in range(len(matches) + 1):\n",
    "    results += combinations(matches, i)\n",
    "\n",
    "  seen = set()\n",
    "  covers = []\n",
    "  size = 999999\n",
    "  min_cover = None\n",
    "\n",
    "  for comb in results:\n",
    "    seen = set()\n",
    "    for s in comb:\n",
    "      term = False\n",
    "      for el in s[0]:\n",
    "        if el in seen:\n",
    "          term = True\n",
    "          break\n",
    "        seen.add(el)\n",
    "      if (term):\n",
    "        break\n",
    "    if len(seen) == num_atoms_in_mol:\n",
    "      covers.append(comb)\n",
    "      # print(len(comb))\n",
    "      if len(comb) < size:\n",
    "        min_cover = comb\n",
    "        size = len(min_cover)\n",
    "  \n",
    "  # print(covers)\n",
    "  return min_cover\n"
   ]
